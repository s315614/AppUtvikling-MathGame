<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mappe1.mathspill">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sign"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sign"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:hardwareAccelerated="false">
        <activity android:name="com.example.mappe1.mathspill.Hoved">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mappe1.mathspill.Spill" />
        <activity android:name="com.example.mappe1.mathspill.Preferenser" />
        <activity android:name="com.example.mappe1.mathspill.Resultat" />

        <activity android:name="com.example.mappe1.mathspill.Statistikk"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>